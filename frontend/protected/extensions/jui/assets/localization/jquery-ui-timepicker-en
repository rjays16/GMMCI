/* Afrikaans translation for the jQuery Timepicker Addon */
/* Written by Deon Heyns */
(function($) {
	$.timepicker.regional['en'] = {
		timeOnlyTitle: 'Time Only',
		timeText: 'Time',
		hourText: 'Hour ',
		minuteText: 'Minute',
		secondText: 'Seconds',
		millisecText: 'Milliseconds',
		timezoneText: 'Timezone',
		currentText: 'Now',
		closeText: 'Done',
		timeFormat: 'hh:mm tt',
		amNames: ['AM', 'A'],
		pmNames: ['PM', 'P'],
		ampm: false
	};
	$.timepicker.setDefaults($.timepicker.regional['en']);
})(jQuery);
